{
    "name": "makville/cakephp-storage-plugin",
    "description": "MakvilleStorage plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "cakephp/cakephp": "^3.5"
    },
    "require": {
        "aws/aws-sdk-php": "^3.24"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.14|^6.0"
    },
    "autoload": {
        "psr-4": {
            "MakvilleStorage\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MakvilleStorage\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
